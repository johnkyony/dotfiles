#!/usr/bin/expect -f
  set timeout 20
	set Username "rs5rack2"
	set Password "foment"
  set Device [lindex $argv 0]
	spawn telnet racks.ine.com
	expect "Username: "
	send "$Username\r"
	expect "Password: "
	send "$Password\r"
  expect "Enter Selection: "
  send "$Device\r\r\r"
  send "\r"
	interact
exit
